{
  "hosting": [
    {
      "target": "capybara-dev-site",
      "public": "frontend/dist",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [{ "source": "**", "destination": "/index.html" }]
    },
    {
      "target": "capybara-main-site",
      "public": "frontend/dist",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [{ "source": "**", "destination": "/index.html" }],
      "redirects": [
        {
          "source": "https://www.doctorcapy.com/**",
          "destination": "https://doctorcapy.com/**",
          "type": 301
        }
      ]
    }
  ]
}
